<div id="mainWrapper">
    <div id="navDiv">
        <nav>
            <div class="nav-links nav-active ul">
                <div id="logoContainer">
                    <div class="logo"></div>
                </div>
                <div class="li" id="loginGuest">Login
                </div>
                <div id="guestContainer">
                    <div class="guestWrapper">
                        <h1 class="guestTitle">Guest</h1>
                        <form id="guestForm" method="POST">
                            <input id="guestUsername" type="text" placeholder="Display Name">
                        </form>
                        <div class="li" id="loginButton">Login</div>
                    </div>
                </div>
                <div class="li" id="channelTitle">
                    <div class="a" id="channelContainer">
                        <p>Channel</p>
                        <div class="channels">
                            <div class="channel">Lobby</div>
                            <div class="channel">Fun</div>
                            <div class="channel">Work</div>
                        </div>
                    </div>
                </div>
                <div id="themesDropdownContainer">
                    <div id="themesDropdownButton">
                        <p>Themes</p>
                    </div>
                    <div id="themesContainer" class="themeContent">
                        <div class="themes" data-theme="default" id="defaultTheme"></div>
                        <div class="themes" data-theme="space" id="spaceTheme"></div>
                        <div class="themes" data-theme="jungle" id="jungleTheme"></div>
                        <div class="themes" data-theme="medieval" id="medievalTheme"></div>
                        <div class="themes" data-theme="urban" id="urbanTheme"></div>
                        <div class="themes" data-theme="food" id="foodTheme"></div>
                        <div class="themes" data-theme="ocean" id="oceanTheme"></div>
                    </div>
                </div>
            </div>
        </nav>
    </div>
    <div id="containLeft">
        <img src="/images/funny_profile.jpg" class="profileImg">
        <h2 class="nameDisplay">Name</h2>
        <div class="channelCurrent"></div>
    </div>
    <div id="conversationBoxWrapper">
        <div class="messageBox">
            <form class="chat">
                <input class="mesgChat" />
                <div id="sendText">Send</div>
            </form>
        </div>
    </div>
</div>
